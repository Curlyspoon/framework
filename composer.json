{
  "name": "curlyspoon/framework",
  "description": "The CurlySpoon Framework.",
  "keywords": [
    "framework",
    "curlyspoon"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Witkowski",
      "email": "dev.gummibeer@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "dragonmantank/cron-expression": "^2.0",
    "laravel/lumen-framework": "5.6.*",
    "symfony/options-resolver": "^4.0",
    "vlucas/phpdotenv": "~2.2"
  },
  "autoload": {
    "psr-4": {
      "Curlyspoon\\Framework\\": "src/"
    },
    "files": [
      "src/Libs/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
